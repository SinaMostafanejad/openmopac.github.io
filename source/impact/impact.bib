	date-modified = {2021-04-18 14:57:09 -0400},
	doi = {https://doi.org/10.1016/j.atmosenv.2021.118373},
	issn = {1352-2310},
	journal = {Atmospheric Environment},
	keywords = {Nucleation mechanism, Amino acid, Topological analysis, Evaporation rate, Atmospheric relevance},
	pages = {118373},
	title = {Valine involved sulfuric acid-dimethylamine ternary homogeneous nucleation and its atmospheric implications},
	url = {https://www.sciencedirect.com/science/article/pii/S1352231021001928},
	year = {2021},
