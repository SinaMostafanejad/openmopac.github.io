@article{LI2021112944,
	abstract = {With serious air pollution, heavy use of asthma drugs caused the accumulation in environmental water, which might pose a potential risk on the organisms. In this study, the ultraviolet photodegradation of asthma drug salmeterol (SX) and influencing factors were studied, meanwhile, the possible photolysis pathway and toxicity trends during the photolysis process were further discussed. The results showed that the photolysis of SX conformed to the pseudo-first order kinetic reaction and 1O2 dominated the photolysis process with contribution of 41.7 %. As the temperature, pH value and HCO3− concentration increased, SX photolysis rate accelerated. The presence of NO3− could also promote the SX photolysis rate but with to Cl2− the increase of NO3− concentration, the promotion reduced via OH quenching and stronger optical shielding. When Cl− concentration increased to 0.1 mM, the promotion effect of Cl− turned to inhibition due to the transformation of Cl to Cl2−. Moreover, the photolysis of SX was inhibited by the Fulvic acid (FA) through optical screening effect and free radical scavenging. Based on the nine intermediates predicted by UPLC-Q-TOF, three possible transformation pathways were proposed. Luminous intensity test of photobacterium phoshoreum T3 spp indicated the toxicity of the final reaction system was reduced through the ultraviolet irradiation.},
	author = {Ping Li and Pu Ge and Senwen Ping and Wenting Lin and Xiaohan Zhang and Chaohai Wei and Yuan Ren},
	date-added = {2021-04-18 19:15:12 -0400},
