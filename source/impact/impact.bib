	issn = {0927-0256},
	journal = {Computational Materials Science},
	keywords = {Microstructure, Grain growth, Grain boundary energy, Grain boundary mobility, Phase-field simulation},
	pages = {109992},
	title = {Large-scale phase-field study of anisotropic grain growth: Effects of misorientation-dependent grain boundary energy and mobility},
	url = {https://www.sciencedirect.com/science/article/pii/S0927025620304833},
	volume = {186},
	year = {2021},
