	journal = {Journal of Photochemistry and Photobiology A: Chemistry},
	keywords = {Salmeterol, Ultraviolet photolysis, Photolysis mechanism, Influencing factors, Photolysis pathway},
	pages = {112944},
	title = {Photodegradation mechanism and influencing factors of asthma drug salmeterol under UV irradiation},
	url = {https://www.sciencedirect.com/science/article/pii/S1010603020307413},
	volume = {404},
