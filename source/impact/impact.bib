	journal = {Computational Materials Science},
	keywords = {Molecular modelling, Zwitterionic molecules, Biomaterials, Surface modification},
	pages = {110418},
	title = {The evaluation of anchoring processes and chemical stability of zwitterionic molecules via local reactivity indexes},
	url = {https://www.sciencedirect.com/science/article/pii/S0927025621001439},
