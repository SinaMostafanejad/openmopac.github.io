	issn = {0013-4686},
	journal = {Electrochimica Acta},
	keywords = {Carbon nitride, DFT theoretical modelling, Electrosynthesis, One-step synthesis, Polypyrrole, Supercapacitor},
	pages = {137570},
	title = {Carbon nitride/polypyrrole composite supercapacitor: Boosting performance and stability},
