	doi = {10.3390/molecules26040797},
	issn = {1420-3049},
	journal = {Molecules},
	number = {4},
	pubmedid = {33557115},
	title = {Combining Different Docking Engines and Consensus Strategies to Design and Validate Optimized Virtual Screening Protocols for the SARS-CoV-2 3CL Protease},
