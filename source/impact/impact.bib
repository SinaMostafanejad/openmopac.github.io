	number = {2},
	pages = {e4979},
	title = {UHPLC/GC-TOF-MS metabolomics, MTT assay, and molecular docking studies reveal physostigmine as a new anticancer agent from the ethyl acetate and butanol fractions of Kigelia africana (Lam.) Benth. fruit extracts},
