	doi = {10.3390/chemistry3010010},
	issn = {2624-8549},
	journal = {Chemistry},
	number = {1},
	pages = {126--137},
	title = {Understanding Conformational Polymorphism in Ganciclovir: A Holistic Approach},
	url = {https://www.mdpi.com/2624-8549/3/1/10},
	volume = {3},
	year = {2021},
