	doi = {10.1109/TNANO.2021.3058408},
	issn = {1941-0085},
	journal = {IEEE Transactions on Nanotechnology},
	keywords = {Logic gates;Dielectrics;Thin film transistors;Threshold voltage;Substrates;Voltage measurement;Transistors;trap-limited conduction;field-effect performance;the density of states;room-temperature TFT;high dielectrics;Sm 2 O 3},
	pages = {1-1},
	title = {Correlating the density of trap-states and the field-effect performance in metal-oxide thin-film transistors with high-k gate dielectrics via a trap-limited conduction method},
