	journal = {Journal of Molecular Liquids},
	keywords = {Ionic liquids, Alkylation, Electronic properties, Isobutane, Molecular dynamics},
	pages = {115411},
	title = {Characterizing the electronic structure of ionic liquid/benzene catalysts for the isobutane alkylation},
