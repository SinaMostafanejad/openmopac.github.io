	journal = {ELECTROPHORESIS},
	keywords = {18-Crown-6, Aromatic amino acids, Chiral separation, Cyclodextrins, Molecular dynamics},
	title = {Capillary electrophoresis and molecular modeling of the chiral separation of aromatic amino acids using α/β-cyclodextrin and 18-crown-6},
