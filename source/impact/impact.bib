	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/anie.202011941},
	journal = {Angewandte Chemie International Edition},
	keywords = {automation, reaction mechanisms, sampling, transition states},
	number = {8},
	pages = {4266-4274},
	title = {autodE: Automated Calculation of Reaction Energy Profiles--- Application to Organic and Organometallic Reactions},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/anie.202011941},
	volume = {60},
