	doi = {https://doi.org/10.1016/j.biopha.2020.111132},
	issn = {0753-3322},
	journal = {Biomedicine & Pharmacotherapy},
	keywords = {In-silico design, Quantum cheminformatics, DNA gyrase, Spirooxindole, Benzimidazoquinazolines, Bactericidal, Biofilm},
	pages = {111132},
	title = {In-silico driven design and development of spirobenzimidazo-quinazolines as potential DNA gyrase inhibitors},
