	issn = {0045-6535},
	journal = {Chemosphere},
	keywords = {chemical risk assessment, ecotoxicity, organic toxicants, similarity-based methods, methods},
	pages = {130681},
	title = {Generating accurate in silico predictions of acute aquatic toxicity for a range of organic chemicals: Towards similarity-based machine learning methods},
	url = {https://www.sciencedirect.com/science/article/pii/S0045653521011528},
	year = {2021},
