	issn = {1386-9477},
	journal = {Physica E: Low-dimensional Systems and Nanostructures},
	keywords = {BN nanocage, Imidacloprid, DFT calculations, Chemisorption, Homonuclear bonds},
	pages = {114456},
	title = {B12N12 nanocages with homonuclear bonds as a promising material in the removal/degradation of the insecticide imidacloprid},
