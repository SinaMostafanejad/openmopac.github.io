	issn = {0269-7491},
	journal = {Environmental Pollution},
	keywords = {QSAR, Mixture toxicity, Toxicology, Joint toxicity, Binary mixture},
	pages = {115885},
	title = {Toxicity of binary mixtures of pesticides and pharmaceuticals toward Vibrio fischeri: Assessment by quantitative structure-activity relationships},
