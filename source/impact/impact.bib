	issn = {2405-8440},
	journal = {Heliyon},
	keywords = {Gaussian 09W, design, Malaria, , QSAR, Screening},
	number = {5},
	pages = {e07032},
	title = {The use of minimal topological differences to inspire the design of novel tetrahydroisoquinoline analogues with antimalarial activity},
	url = {https://www.sciencedirect.com/science/article/pii/S240584402101135X},
	volume = {7},
	year = {2021},
