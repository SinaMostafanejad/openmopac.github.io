	doi = {https://doi.org/10.1016/j.jddst.2020.102103},
	issn = {1773-2247},
	journal = {Journal of Drug Delivery Science and Technology},
	keywords = {, Gingerol, Cyclodextrin, NMR, Dissolution test, Cytotoxicity},
	pages = {102103},
