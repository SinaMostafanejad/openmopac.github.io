	journal = {Molecules},
	number = {7},
	title = {MetaTREE, a Novel Database Focused on Metabolic Trees, Predicts an Important Detoxification Mechanism: The Glutathione Conjugation},
	url = {https://www.mdpi.com/1420-3049/26/7/2098},
