	keywords = {Cubane, Cubanoid, DFT and TD-DFT methods, Optoelectronic properties, Thermodynamic properties},
	pages = {107820},
	title = {Cubane and cubanoid: Structural, optoelectronic and thermodynamic properties from DFT and TD-DFT method},
