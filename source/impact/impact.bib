	journal = {International Journal of Biological Macromolecules},
	keywords = {GH11 xylanase, Xylooligosaccharides, Molecular dynamics simulations, Protein engineering},
	pages = {240-247},
	title = {Effect of N-terminal modification on the mode of action between the Xyn11A and Xylotetraose},
