	isbn = {1572-9001},
	journal = {Structural Chemistry},
	number = {2},
	pages = {581--589},
	title = {Efficient approach for exploring the multiple-channel bimolecular interactions of conformationally flexible reagents. Epoxide ring opening reaction},
