	doi = {https://doi.org/10.1016/j.ijpharm.2021.120284},
	issn = {0378-5173},
	journal = {International Journal of Pharmaceutics},
	keywords = {Tribocharging, Work function, Hopper flow, Discrete Element Modeling},
	pages = {120284},
	title = {Discrete Element Modeling (DEM) based investigation of tribocharging in the pharmaceutical powders during hopper discharge},
	url = {https://www.sciencedirect.com/science/article/pii/S0378517321000880},
	volume = {596},
	year = {2021},
	Bdsk-Url-1 = {https://www.sciencedirect.com/science/article/pii/S0378517321000880},
