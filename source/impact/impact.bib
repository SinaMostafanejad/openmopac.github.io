	issn = {1010-6030},
	journal = {Journal of Photochemistry and Photobiology A: Chemistry},
	keywords = {Photo-bactericidal, , Singlet oxygen, Graphene oxide, UV-vis irradiation},
	pages = {113058},
	title = {Photochemistry of covalently bonded graphene oxide -- Perylene diimide system for bacterial growth inhibition started by singlet oxygen},
	url = {https://www.sciencedirect.com/science/article/pii/S1010603020308558},
