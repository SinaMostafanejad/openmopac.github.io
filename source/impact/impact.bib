	issn = {0048-9697},
	journal = {Science of The Total Environment},
	keywords = {Halobenzoquinones, Drinking water, Disinfection byproducts, Occurrence, Formation, Toxicity},
	pages = {145277},
	title = {Occurrence and toxicity of halobenzoquinones as drinking water disinfection byproducts},
	url = {https://www.sciencedirect.com/science/article/pii/S0048969721003430},
	volume = {770},
	year = {2021},
