	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/php.13337},
	journal = {Photochemistry and Photobiology},
	number = {2},
	pages = {289-300},
	title = {Simple 3,6-disubstituted Carbazoles as Potential Hole Transport Materials: Photophysical, Electrochemical and Theoretical Studies},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/php.13337},
