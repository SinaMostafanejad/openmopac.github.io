	date-modified = {2021-04-22 12:12:51 -0400},
	doi = {https://doi.org/10.1016/j.molliq.2021.116023},
	issn = {0167-7322},
	journal = {Journal of Molecular Liquids},
	keywords = {Naphthalenediimide, Cucurbiturils, Host-guest complexes, Excimer emission, Anion radical emission, Gold nanoparticles},
	pages = {116023},
	title = {Photoinduced emissive naphthalenediimide radical anion in the confinement of cucurbituril nanocavity; in situ generation of gold nanoparticles},
