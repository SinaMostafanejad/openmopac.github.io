	issn = {0141-0229},
	journal = {Enzyme and Microbial Technology},
	keywords = {Amino acid, β-Glucosidase, Catalysis, Molecular docking, -NPG, Food industry},
	pages = {109814},
	title = {Exploring the catalytic mechanism of a novel β-glucosidase BGL0224 from Oenococcus oeni SD-2a: Kinetics, spectroscopic and molecular simulation},
	url = {https://www.sciencedirect.com/science/article/pii/S0141022921000727},
