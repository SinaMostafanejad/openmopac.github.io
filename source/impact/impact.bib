	doi = {https://doi.org/10.1016/j.bioorg.2021.104631},
	issn = {0045-2068},
	journal = {Bioorganic Chemistry},
	keywords = {DNA, SN38 derivatives, Metabolites, NMR},
	pages = {104631},
	title = {A NMR study of binding the metabolite of SN38 derivatives to a model nicked DNA decamer mimicking target of Topo I inhibitors},
	url = {https://www.sciencedirect.com/science/article/pii/S0045206821000079},
