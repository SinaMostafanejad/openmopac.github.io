	journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
	keywords = {Phosphoramidate, Amphiphiles, Monolayer, Surface tension, Carbohydrates},
	pages = {125867},
	title = {Synthesis and interfacial properties of glyco-lipophosphoramidates},
	url = {https://www.sciencedirect.com/science/article/pii/S0927775720314606},
	volume = {611},
