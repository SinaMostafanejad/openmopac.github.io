	keywords = {Catalytic hydrogen transfer, Cascade reaction, Imine synthesis, MOF-derived carbon material, Single atom catalysis},
	pages = {120288},
	title = {Nano-pyramid-type Co-ZnO/NC for Hydrogen Transfer Cascade Reaction between Alcohols and Nitrobenzene},
	url = {https://www.sciencedirect.com/science/article/pii/S0926337321004148},
