	pages = {4810-4822},
	publisher = {The Royal Society of Chemistry},
	title = {An imine linked fluorescent covalent organic cage: the sensing of chloroform vapour and metal ions{,} and the detection of nitroaromatics},
	url = {http://dx.doi.org/10.1039/D1NJ00434D},
