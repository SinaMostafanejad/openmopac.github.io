	doi = {https://doi.org/10.1016/j.jhazmat.2021.125495},
	issn = {0304-3894},
	journal = {Journal of Hazardous Materials},
	keywords = {Quinolone, Chlorine disinfection, In silico method, Antibiotic activity, Genotoxicity},
	pages = {125495},
	title = {Bioassay- and QSAR-based screening of toxic transformation products and their formation under chlorination treatment on levofloxacin},
	url = {https://www.sciencedirect.com/science/article/pii/S0304389421004581},
	volume = {414},
