	date-modified = {2021-04-18 15:40:07 -0400},
	doi = {10.1039/D0RA09418H},
	issue = {8},
	journal = {RSC Adv.},
	pages = {4444-4453},
	publisher = {The Royal Society of Chemistry},
	title = {Microwave-assisted synthesis of ruthenium(ii) complexes containing levofloxacin-induced G2/M phase arrest by triggering DNA damage},
	url = {http://dx.doi.org/10.1039/D0RA09418H},
	volume = {11},
	year = {2021},
