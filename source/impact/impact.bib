	issn = {2073-4433},
	journal = {Atmosphere},
	number = {3},
	title = {Measured Solid State and Sub-Cooled Liquid Vapour Pressures of Benzaldehydes Using Knudsen Effusion Mass Spectrometry},
	url = {https://www.mdpi.com/2073-4433/12/3/397},
	volume = {12},
