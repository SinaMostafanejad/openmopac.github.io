	doi = {https://doi.org/10.1002/jccs.202000158},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/jccs.202000158},
	journal = {Journal of the Chinese Chemical Society},
	keywords = {1HNJ, antibiotic, docking, FM-DNA, in silico, in vitro, nanoparticle},
