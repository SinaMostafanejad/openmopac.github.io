	doi = {10.3390/polym13081249},
	issn = {2073-4360},
	journal = {Polymers},
	number = {8},
	title = {Polymer Adsorbents vs. Functionalized Oxides and Carbons: Particulate Morphology and Textural and Surface Characteristics},
	url = {https://www.mdpi.com/2073-4360/13/8/1249},
	volume = {13},
