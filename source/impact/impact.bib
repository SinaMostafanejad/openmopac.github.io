	keywords = {Carbonaceous material, Oxygen-containing functional group, Mesoporous and external surface, Adsorption capacity, Predictive quantitative correlation},
	pages = {130021},
	title = {Mechanisms of aromatic molecule - Oxygen-containing functional group interactions on carbonaceous material surfaces},
	url = {https://www.sciencedirect.com/science/article/pii/S0045653521004902},
	volume = {275},
