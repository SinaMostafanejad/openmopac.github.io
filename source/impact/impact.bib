	doi = {https://doi.org/10.1016/j.chemolab.2020.104213},
	issn = {0169-7439},
	journal = {Chemometrics and Intelligent Laboratory Systems},
	keywords = {hERG blockers, QSAR, Machine learning, MACCS Fingerprints, MOE-Descriptors, Support vector machine},
	pages = {104213},
	title = {Multi-algorithm based machine learning and structural pattern studies for hERG ion channel blockers mediated cardiotoxicity prediction},
